   ],
   "source": [
    "import os\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import scipy.optimize as spo\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline \n",
    "\n",
    "from sklearn.cross_validation import train_test_split\n",
    "from sklearn import linear_model\n",
    "\n",
    "\n",
    "def symbol_to_path(symbol, base_dir=\"data\"):\n",
    "    return os.path.join(base_dir, \"{}.csv\".format(str(symbol)))\n",
    "\n",
    "\n",
    "def plot_selected(df, columns, start_index, end_index):\n",
    "\n",
    "    plot_data(df.ix[start_index:end_index,columns], title=\"SPY\")\n",
    "    \n",
    "    \n",
    "def plot_data(df, title=\"Stock prices\"):\n",
    "    ax = df.plot(title=title,fontsize=12,figsize=(12,10))\n",
    "    ax.set_xlabel(\"Date\")\n",
    "    ax.set_ylabel(\"Price\")\n",
    "    plt.legend(bbox_to_anchor=(1.05, 1), loc=2, borderaxespad=0.)\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "def show_supervised_linear_regression():\n",
    "    \n",
    "    # dates = pd.date_range('2015-01-01', '2016-01-01') # Define date range\n",
    "    # df = pd.DataFrame(index=dates)\n",
    "    \n",
    "    df = pd.read_csv(symbol_to_path('SPY'), index_col='Date', \n",
    "                parse_dates=True, \n",
    "                usecols=['Date', 'Open', 'High', 'Low', 'Close', 'Volume', 'Adj Close'], \n",
    "                na_values=['nan'])\n",
    "    \n",
    "    df = df.sort_index(ascending=True, axis=0)\n",
    "    \n",
    "    # print \"df: \"\n",
    "    # print df.head()\n",
    "    # print \"\\n\"\n",
    "    # print df.tail()\n",
    "    # print \"\\n\"\n",
    "    \n",
    "    # df.fillna(method=\"ffill\", inplace=\"True\") # Forward fill empty trade dates (for AXY)\n",
    "    # df.fillna(method=\"bfill\", inplace=\"True\") # backfill empty trade dates (for AXY)\n",
    "    \n",
    "    # df.plot(figsize=(12,10))\n",
    "    # plt.title(\"SPY performance\")\n",
    "    # plt.show()\n",
    "    \n",
    "    # Slice and plot\n",
    "    plot_selected(df, ['Adj Close'], '2015-01-01', '2016-01-01')\n",
    "\n",
    "    \n",
    "    # columns = ['Date', 'Open', 'High', 'Low', 'Close', 'Volume', 'Adj Close']\n",
    "    # columns = ['Date', 'Adj Close']\n",
    "    # df.ix['2015-01-01':'2016-01-01', columns]\n",
    "    # ax = df.plot(title=\"SPY\",fontsize=12,figsize=(12,10))\n",
    "    # ax.set_xlabel(\"Date\")\n",
    "    # ax.set_ylabel(\"Price\")\n",
    "    # plt.legend(bbox_to_anchor=(1.05, 1), loc=2, borderaxespad=0.)\n",
    "    # plt.show()\n",
    "    \n",
    "    # Get the features and labels from the stock dataset\n",
    "    X = df.iloc[:,:-1]\n",
    "    y = df.iloc[:, -1]\n",
    "    \n",
    "    # Split the data into training/testing sets\n",
    "    X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=0)\n",
    "    ## note:  roll forward cross validation?\n",
    "    \n",
    "    print \"X_train: \"\n",
    "    print X_train.head()\n",
    "    print \"\\n\"\n",
    "    print \"X_test: \"\n",
    "    print X_test.head()\n",
    "    print \"\\n\"\n",
    "    print \"y_train: \"\n",
    "    print y_train.head()\n",
    "    print \"\\n\"\n",
    "    print \"y_test\"\n",
    "    print y_test.head()\n",
    "    print \"X shape: \"\n",
    "    print X.shape\n",
    "    print \"\\n\"\n",
    "    print \"y shape: \"\n",
    "    print y.shape\n",
    "    print \"\\n\"\n",
    "    \n",
    "    # Create linear regression object\n",
    "    regr = linear_model.LinearRegression()\n",
    "    \n",
    "    # Train the model using the training sets\n",
    "    regr.fit(X_train, y_train)\n",
    "    \n",
    "    # The coefficients\n",
    "    # print('Coefficients: \\n', regr.coef_)\n",
    "    print \"Coefficients: \\n\", regr.coef_\n",
    "\n",
    "    # The mean square error\n",
    "    print(\"Residual sum of squares: %.2f\"\n",
    "          % np.mean((regr.predict(X_test) - y_test) ** 2))\n",
    "\n",
    "    # Explained variance score: 1 is perfect prediction\n",
    "    print('Variance score: %.2f' % regr.score(X_test, y_test))\n",
    "\n",
    "    # Plot outputs\n",
    "    plt.figure(figsize=(12,10))\n",
    "    plt.scatter(X_test, y_test,  color='black')\n",
    "    plt.plot(X_test, regr.predict(X_test), color='blue', linewidth=3)\n",
    "\n",
    "    plt.xticks(())\n",
    "    plt.yticks(())\n",
    "\n",
    "    plt.show()\n",
    "\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    show_supervised_linear_regression()"
   ]
  },
